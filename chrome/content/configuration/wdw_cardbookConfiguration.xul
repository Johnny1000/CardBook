	<prefpane id="CardBookTab" label="&CardBookTabLabel;" index="CardBook">

		<preferences id="cardbookCardBookPreferences">
			<preference id="extensions.cardbook.mailPopularityTabView"
				name="extensions.cardbook.mailPopularityTabView" type="bool"/>
			<preference id="extensions.cardbook.technicalTabView"
				name="extensions.cardbook.technicalTabView" type="bool"/>
			<preference id="extensions.cardbook.vcardTabView"
				name="extensions.cardbook.vcardTabView" type="bool"/>
			<preference id="extensions.cardbook.advancedTabView"
				name="extensions.cardbook.advancedTabView" type="bool"/>
			<preference id="extensions.cardbook.localizeEngine"
				name="extensions.cardbook.localizeEngine" type="string"/>
			<preference id="extensions.cardbook.showNameAs"
				name="extensions.cardbook.showNameAs" type="string"/>
			<preference id="extensions.cardbook.adrFormula"
				name="extensions.cardbook.adrFormula" type="string"/>
			<preference id="extensions.cardbook.localizeTarget"
				name="extensions.cardbook.localizeTarget" type="string"/>
			<preference id="extensions.cardbook.preferEmailEdition"
				name="extensions.cardbook.preferEmailEdition" type="bool"/>
			<preference id="extensions.cardbook.dateDisplayedFormat"
				name="extensions.cardbook.dateDisplayedFormat" type="string"/>
		</preferences>
		
		<groupbox>
			<caption label="&tabsEditingGroupboxLabel;"/>
			<hbox align="center">
				<checkbox id="mailPopularityTabCheckBox" label="&mailPopularityTabLabel;"
					accesskey="&mailPopularityTabAccesskey;"
					preference="extensions.cardbook.mailPopularityTabView"/>
				<checkbox id="technicalTabCheckBox" label="&technicalTabLabel;"
					accesskey="&technicalTabAccesskey;"
					preference="extensions.cardbook.technicalTabView"/>
				<checkbox id="vcardTabCheckBox" label="&vCardTabLabel;"
					accesskey="&vCardTabAccesskey;"
					preference="extensions.cardbook.vcardTabView"/>
				<checkbox id="advancedTabCheckBox" label="&advancedTabLabel;"
					accesskey="&advancedTabAccesskey;"
					preference="extensions.cardbook.advancedTabView"/>
			</hbox>
		</groupbox>

		<groupbox>
			<caption label="&localizeEditingGroupboxLabel;"/>
			<hbox flex="1" align="center">
				<radiogroup id="localizeEngineRadiogroup" orient="horizontal" preference="extensions.cardbook.localizeEngine">
					<radio value="OpenStreetMap" label="&localizeEngineOpenStreetMapLabel;" accesskey="&localizeEngineOpenStreetMapAccesskey;"/>
					<radio value="GoogleMaps" label="&localizeEngineGoogleMapsLabel;" accesskey="&localizeEngineGoogleMapsAccesskey;"/>
					<radio value="BingMaps" label="&localizeEngineBingMapsLabel;" accesskey="&localizeEngineBingMapsAccesskey;"/>
				</radiogroup>
			</hbox>
		</groupbox>

		<groupbox>
			<caption label="&showNameAsLabel;"/>
			<hbox flex="1" align="center">
				<radiogroup id="showNameAsRadiogroup" orient="horizontal" preference="extensions.cardbook.showNameAs">
					<radio value="LF" label="&showNameAsLFLabel;" accesskey="&showNameAsLFAccesskey;"/>
					<radio value="LFCOMMA" label="&showNameAsLFCommaLabel;" accesskey="&showNameAsLFCommaAccesskey;"/>
					<radio value="FL" label="&showNameAsFLLabel;" accesskey="&showNameAsFLAccesskey;"/>
					<radio value="DSP" label="&showNameAsDSPLabel;" accesskey="&showNameAsDSPAccesskey;"/>
				</radiogroup>
			</hbox>
		</groupbox>

		<groupbox>
			<caption label="&adrFormulaLabel;"/>
			<description>&adrLabelExplanation;</description>
			<grid align="center" flex="1">
				<columns>
					<column/>
					<column flex="1"/>
					<column/>
				</columns>

				<rows>
					<row align="center">
						<label value="&adrFormulaLabel;"
							accesskey="&adrFormulaAccesskey;"
							control="adrFormulaTextBox"
							id="adrFormula"/>
						<textbox id="adrFormulaTextBox"/>
						<button id="resetAdrFormulaButton" label="&resetAdrFormulaLabel;" accesskey="&resetAdrFormulaAccesskey;"
							oncommand="wdw_cardbookConfiguration.resetAdrFormula();"/>
					</row>
					<row align="center">
						<label value=""/>
						<label id="adrFormulaDescriptionLabel1"/>
						<label value=""/>
					</row>
					<row align="center">
						<label value=""/>
						<label id="adrFormulaDescriptionLabel2"/>
						<label value=""/>
					</row>
				</rows>
			</grid>
		</groupbox>

		<groupbox>
			<caption label="&localizeTargetLabel;"/>
			<hbox flex="1" align="center">
				<radiogroup id="localizeTargetRadiogroup" orient="horizontal" preference="extensions.cardbook.localizeTarget">
					<radio value="in" label="&localizeTargetInLabel;" accesskey="&localizeTargetInAccesskey;"/>
					<radio value="out" label="&localizeTargetOutLabel;" accesskey="&localizeTargetOutAccesskey;"/>
				</radiogroup>
			</hbox>
		</groupbox>

		<groupbox>
			<caption label="&miscEditingGroupboxLabel;"/>
			<hbox flex="1" align="center">
				<checkbox id="preferEmailEditionCheckBox" label="&preferEmailEditionLabel;"
					accesskey="&preferEmailEditionAccesskey;"
					preference="extensions.cardbook.preferEmailEdition"/>
			</hbox>
			<separator class="thin"/>
			<hbox flex="1" align="center">
				<label value="&dateDisplayedFormatLabel;" accesskey="&dateDisplayedFormatAccesskey;" control="dateDisplayedFormatMenulist"/>
				<menulist id="dateDisplayedFormatMenulist" crop="none" preference="extensions.cardbook.dateDisplayedFormat">
					<menupopup id="dateDisplayedFormatMenupopup">
						<menuitem id="dateDisplayedFormatLong" value="0"/>
						<menuitem id="dateDisplayedFormatShort" value="1"/>
					</menupopup>
				</menulist>
			</hbox>
		</groupbox>
	</prefpane>

	<prefpane id="typesTab" label="&typesTabLabel;" index="types">

		<preferences id="cardbookTypesPreferences">
			<preference id="extensions.cardbook.usePreferenceValue"
				name="extensions.cardbook.usePreferenceValue" type="bool"/>
			<preference id="extensions.cardbook.preferenceValueLabel"
				name="extensions.cardbook.preferenceValueLabel" type="string"/>
		</preferences>


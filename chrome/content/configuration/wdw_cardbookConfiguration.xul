					preference="extensions.cardbook.exclusive"/>
			</hbox>
			<separator class="thin"/>
			<vbox>
				<checkbox id="autocompletionCheckBox" label="&autocompletionLabel;"
					accesskey="&autocompletionAccesskey;"
					tooltiptext = "&autocompletionTooltip;"
					preference="extensions.cardbook.autocompletion"
					oncommand="wdw_cardbookConfiguration.cardbookAutoComplete();"/>
				<separator class="thin"/>
				<checkbox id="autocompleteSortByPopularityCheckBox" label="&autocompleteSortByPopularityLabel;"
					accesskey="&autocompleteSortByPopularityAccesskey;"
					class="indent"
					preference="extensions.cardbook.autocompleteSortByPopularity"/>
				<separator class="thin"/>
				<checkbox id="autocompleteProposeConcatEmailsCheckBox" label="&autocompleteProposeConcatEmailsLabel;"
					accesskey="&autocompleteProposeConcatEmailsAccesskey;"
					class="indent"
					preference="extensions.cardbook.proposeConcatEmails"/>
				<separator class="thin"/>
				<checkbox id="autocompleteShowAddressbookCheckBox" label="&autocompleteShowAddressbookLabel;"
					accesskey="&autocompleteShowAddressbookAccesskey;"
					class="indent"
					preference="extensions.cardbook.autocompleteShowAddressbook"/>
				<separator class="thin"/>
				<checkbox id="autocompleteWithColorCheckBox" label="&autocompleteWithColorLabel;"
					accesskey="&autocompleteWithColorAccesskey;"
					class="indent"
					preference="extensions.cardbook.autocompleteWithColor"/>

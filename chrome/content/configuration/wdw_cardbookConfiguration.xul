		<groupbox flex="1">
			<caption label="&IMPPGroupboxLabel;"/>
			<hbox align="center">
				<checkbox id="preferIMPPPrefCheckBox" label="&preferIMPPPrefLabel;"
					accesskey="&preferIMPPPrefAccesskey;"
					preference="extensions.cardbook.preferIMPPPref"/>
